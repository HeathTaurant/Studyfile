<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
    <mapper namespace="com.musunsul29.db.MapperEthanol">
	
	<select id="myetList" resultType="com.musunsul29.DTOEthanol">
	
	select * from ethanol AS et join review AS re on et.et_pk=#{et_pk} = re.et_pk=#{et_pk}
	
	</select>
	
	
	<select id="selectEthanol" resultType="com.musunsul29.DTOEthanol">
	
	select * from ethanol where et_name =#{et_name}
	
	</select>
	
	
	<select id="searchEthanol"  parameterType="String" resultType="com.musunsul29.DTOEthanol">
	
	select * from ethanol where et_name like CONCAT('%',#{et_name},'%') 
	</select>
	
		<select id="getEthanol" parameterType="int" resultType="com.musunsul29.DTOEthanol">
	select * from ethanol where et_pk =#{et_pk}  
	</select>

</mapper>